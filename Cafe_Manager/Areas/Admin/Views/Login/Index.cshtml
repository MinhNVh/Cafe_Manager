
@model Cafe_Manager.Areas.Admin.Models.LoginModels

<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">
    <title>Login Coffee Hello</title>
    <!-- Import Bootstrap and JQuery -->
    <script src="https://ajax.googleapis.com/ajax/libs/jquery/3.3.1/jquery.min.js">
    </script>
    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/css/bootstrap.min.css">
    <script src="https://maxcdn.bootstrapcdn.com/bootstrap/4.1.3/js/bootstrap.min.js"></script>
    <link href="~/Content/css/stylelogin.css" rel="stylesheet">
</head>
<body>
    <div class="container-fluid bg">
        <div class="row justify-content-center">
            <div class="col-md-3 col-sm-6 col-xs-12 row-container">
                @using (Html.BeginForm("Login", "Login", FormMethod.Post))
                 {
                    @Html.ValidationSummary(false, "", "error");
                    <form>
                        <h1>Đăng Nhập</h1>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group fontcolor">
                                    <label>Tài Khoản</label>
                                    @Html.TextBoxFor(model => model.UserName, new { @class = "form-control", @autofocus = "autofocus" })
                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-12">
                                <div class="form-group">
                                    <label>Mật Khẩu</label>
                                    
                                    @Html.PasswordFor(model => model.PassWord, new { @class = "form-control" })

                                </div>
                            </div>
                        </div>
                        <div class="row">
                            <div class="col-md-7 font">
                                @Html.ActionLink("Đăng kí", "SignUp", "Login")
                            </div>
                            <div class="col-md-5 font">
                                <button type="submit" class="login100-form-btn">
                                    Login
                                </button>
                                @*@Html.ActionLink("Đăng Nhập", "SanPham", "Admin")*@
                            </div>
                        </div>
                    </form>
                 }
            </div>
                
        </div>
    </div>
</body>
</html>




